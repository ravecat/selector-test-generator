{
  "name": "selector-test-generator",
  "description": "Test generator for selectors based on reselect library",
  "version": "1.2.0",
  "homepage": "https://gitlab.com/ravecat/selector-test-generator",
  "keywords": [
    "reselect",
    "selector",
    "redux"
  ],
  "author": "Max Sharov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ravecat/selector-test-generator"
  },
  "bugs": {
    "url": "https://gitlab.com/ravecat/selector-test-generator/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist/",
    "cli.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "docker run --rm $(docker build -q .)",
    "test:generate": "node dist/index.js ./app -s ./app/state.js",
    "release:debug": "semantic-release --debug",
    "lint": "eslint '**/*.js'",
    "prettier": "prettier --check *"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^8.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/gitlab": "^3.1.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "cz-customizable": "^6.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.18.2",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^15.13.18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,yml,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.config.js"
    }
  },
  "dependencies": {
    "@babel/parser": "^7.6.4",
    "commander": "^3.0.2",
    "outdent": "^0.7.0"
  }
}
